TOPDIR := ../../../
include $(TOPDIR)sdk/build/core/config.mak


all:
	@$(TARGET_CC) -I$(OUT_DIR)/lua/lua-5.1/include -Wall -O2 -shared -o struct.so struct.c
	@$(MKDIR) -p $(OUT_DIR)/lua/clib
	@$(CP) -f struct.so $(OUT_DIR)/lua/clib

clean:
	@$(RM) -rf struct.so
	@$(RM) -rf $(OUT_DIR)/lua/clib/struct.so

