.PHONY: allall: base_cpio.PHONY: base_cpiobase_cpio:	tar xjf busybox/busybox-1.17.3.tar.bz2	cp busybox/config-1.17.3 busybox-1.17.3/.config	cd busybox-1.17.3; \	make CROSS_COMPILE=arm-none-linux-gnueabi-; \	make install CROSS_COMPILE=arm-none-linux-gnueabi-	sudo mkdir busybox-1.17.3/_install/dev	sudo mknod busybox-1.17.3/_install/dev/console c 5 1	sudo mknod busybox-1.17.3/_install/dev/null c 1 3	cd busybox-1.17.3/_install; \	ln -s bin/busybox init; \	find . | cpio -H newc -o --owner root:root -F ../../base.cpio	sudo rm -rf busybox-1.17.3/_install/dev